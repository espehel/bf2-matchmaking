{
  "name": "bot",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf build && esbuild --define:process.env.NODE_ENV=\\\"production\\\" src/app.ts --bundle --platform=node --outfile=build/app.js --external:express  --external:discord.js",
    "start": "node build/app.js",
    "verify": "tsc --noEmit"
  },
  "engines": {
    "node": "18 || 19"
  },
  "dependencies": {
    "@bf2-matchmaking/discord": "*",
    "@bf2-matchmaking/logging": "*",
    "@bf2-matchmaking/supabase": "*",
    "@bf2-matchmaking/types": "*",
    "@bf2-matchmaking/utils": "*",
    "@koa/bodyparser": "^5.0.0",
    "discord-interactions": "^3.4.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.0.0",
    "koa": "^2.14.2",
    "luxon": "^3.4.3"
  },
  "devDependencies": {
    "@types/luxon": "^3.3.3",
    "@types/node": "^18.11.10",
    "esbuild": "^0.17.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
