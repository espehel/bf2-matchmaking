{
  "name": "bot",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf ./build && esbuild src/app.ts --bundle --platform=node --outfile=build/app.js --external:express  --external:discord.js",
    "start": "node build/app.js",
    "verify": "tsc --noEmit"
  },
  "engines": {
    "node": "18 || 19"
  },
  "dependencies": {
    "@bf2-matchmaking/discord": "*",
    "@bf2-matchmaking/logging": "*",
    "@bf2-matchmaking/supabase": "*",
    "@bf2-matchmaking/types": "*",
    "@bf2-matchmaking/utils": "*",
    "discord.js": "^14.13.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "luxon": "^3.4.3",
    "moment": "^2.29.4",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/luxon": "^3.3.3",
    "@types/node": "^18.11.10",
    "esbuild": "^0.17.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
